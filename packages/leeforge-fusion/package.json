{
  "name": "@leeforge/fusion",
  "version": "0.1.0-beta.1",
  "description": "A full-stack SSR framework with file-based routing, middleware, and server actions",
  "type": "module",
  "keywords": [
    "ssr",
    "fullstack",
    "framework",
    "routing",
    "middleware",
    "server-actions",
    "vite",
    "hono",
    "tanstack-router"
  ],
  "author": "YeonGyu Kim <code-yeongyu>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/code-yeongyu/leeforge.git",
    "directory": "packages/leeforge-fusion"
  },
  "bugs": {
    "url": "https://github.com/code-yeongyu/leeforge/issues"
  },
  "homepage": "https://github.com/code-yeongyu/leeforge#readme",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./config": "./dist/config.js",
    "./middleware": "./dist/middleware.js",
    "./data": "./dist/data.js",
    "./client": "./dist/client.js",
    "./vite-plugin": "./dist/vite-plugin.js"
  },
  "files": [
    "dist/",
    "src/",
    "docs/",
    "examples/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts --format esm --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm test"
  },
  "dependencies": {
    "@tanstack/solid-query": "^5.90.20",
    "@tanstack/solid-router": "^1.150.0",
    "compression": "^1.8.1",
    "express": "^5.2.1",
    "glob": "^11.1.0",
    "hono": "^4.11.4",
    "minimatch": "^10.1.1",
    "sirv": "^3.0.2",
    "solid-js": "^1.9.10",
    "vite": "^7.3.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/express": "^5.0.6",
    "@types/glob": "^8.1.0",
    "@types/node": "^24.10.2",
    "typescript": "^5.6.2",
    "tsup": "^8.3.5",
    "vitest": "^2.1.9"
  },
  "peerDependencies": {
    "solid-js": "^1.9.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
