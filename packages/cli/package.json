{
  "name": "@leeforge/cli",
  "version": "0.1.0-beta.1",
  "description": "CLI tool for Leeforge Fusion framework",
  "type": "module",
  "keywords": [
    "leeforge",
    "cli",
    "scaffold",
    "generator",
    "fullstack",
    "framework"
  ],
  "author": "YeonGyu Kim <code-yeongyu>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/code-yeongyu/leeforge.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/code-yeongyu/leeforge/issues"
  },
  "homepage": "https://github.com/code-yeongyu/leeforge#readme",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
