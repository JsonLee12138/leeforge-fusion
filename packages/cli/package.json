    "minimatch": "^10.1.1"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.10.2",
    "tsup": "^8.3.5",
    "typescript": "^5.6.2",
    "vitest": "^2.1.9"
  },
  "engines": {
    "node": ">=18.0.0",
